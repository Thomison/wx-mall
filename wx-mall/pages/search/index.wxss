page {
  background-color: #d6cdcd;
  padding: 20rpx;
}
.search_row {
  height: 90rpx;
  display: flex;
  margin-bottom: 20rpx;
}
.search_row input {
  background-color: #fff;
  flex: 1;
  height: 100%;
  padding-left: 20rpx;
}
.search_row button {
  background-color: #fff;
  font-size: 28rpx;
  margin-left: 15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_content .search_item {
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx ;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

